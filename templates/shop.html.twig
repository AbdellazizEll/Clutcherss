{% extends 'index.html.twig' %}
{% block title %}  Jeux  {% endblock %}
{% block body %}
<body>




	<!-- Games section -->

	<section class="games-section">
		<h1>Nos Produits</h1>

		<div class="container">
			<div class="row">

				<div class="col-xl-7 col-lg-8 col-md-7">
						<div class="row">
							{% for e in prod %}

							<div class="col-lg-4 col-md-6">
							<div class="game-item">

								<img src="/uploads/{{e.image}}" width="150" height="150" alt="#">
								<p class="text-xs font-weight-bold mb-0">{{ e.nom }}</p>
								<p class="text-xs font-weight-bold mb-0">{{ e.description }}</p>
								<p class="text-xs font-weight-bold mb-0">{{ e.prix }} DT</p>



							</div>
						</div>
							{% endfor %}

						</div>
				</div>
			</div>

		</div>
	</section>
	<!-- Games end-->


	

	
	</body>

    {% endblock %}